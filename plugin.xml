<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="metaMap-global-id-sdk-cordova-plugin" version="0.0.4">
   <name>MetaMapGlobalIDSDK</name>
   <js-module name="MetaMapGlobalIDSDK" src="www/MetaMapGlobalIDSDK.js">
      <clobbers target="cordova.plugins.MetaMapGlobalIDSDK" />
   </js-module>
   <platform name="android">
      <config-file parent="/*" target="res/xml/config.xml">
         <feature name="MetaMapGlobalIDSDK">
            <param name="android-package" value="com.cordova.plugin.metaMapglobalidsdk.MetaMapGlobalIDSDK" />
            <param name="onload" value="true" />
         </feature>
      </config-file>
      <config-file parent="/*" target="AndroidManifest.xml" />
      <source-file src="src/android/MetaMapGlobalIDSDK.java" target-dir="src/com/cordova/plugin/metaMapglobalidsdk" />
      <framework src="src/android/build.gradle" custom="true" type="gradleReference" />
      <config-file target="AndroidManifest.xml" parent="/manifest/application">
      </config-file>
   </platform>
   <platform name="ios">
      <dependency id="cordova-plugin-cocoapod-support"/>
      <pods-config ios-min-version="12.0" use-frameworks="true">
      </pods-config>
      <config-file parent="/*" target="config.xml">
         <feature name="MetaMapGlobalIDSDK">
            <param name="ios-package" value="MetaMapGlobalIDSDK" />
            <param name="onload" value="true" />
         </feature>
      </config-file>
      <source-file src="src/ios/MetaMapSDK.m" />
      <pod name="MetaMap-Global-ID-SDK" />
   </platform>
</plugin>
